rt\">\r\n            <mt-field v-model=\"uname\" label=\"账户\" :attr=\"{value:uname}\" :placeholder=\"accountPlaceholder\">\r\n            </mt-field>\r\n        <mt-field :label='label' v-model=\"password\" :type=\"pass\" :attr=\"{value:password}\" :placeholder=\"passwordPlaceholder\"></mt-field>\r\n<button class=\"float-right\" :class=\"buttonss\" @click=\"get\">获得验证码</button>\r\n        </div>\r\n        <div>\r\n                <div class=\"text-align-center\">\r\n                <button @click=\"substr\">登录</button>\r\n                </div>\r\n            <button class=\"my-button\" @click=\"changeLoginMethod\">{{qiehuan}}</button>\r\n        </div>\r\n        </div>\r\n        <div :class=\"dnone\">\r\n            <h3>欢迎{{uname}}用户</h3>\r\n            <button>设置</button>\r\n            <button :class=\"subscr\" @click=\"subcr\">注销</button>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            accountPlaceholder:\"手机/邮箱/会员名\",\r\n            passwordPlaceholder:\"请输入验证码/密码\",\r\n            label:\"密码\",\r\n            isUserAccount:true,\r\n            uname:\"\",\r\n            password:\"\",\r\n            reg:\"\",\r\n            qiehuan:\"账号密码登录\",\r\n            buttonss:{d_none:true,d_block:false,},\r\n            dnone:{d_none:true,d_block:false,},\r\n            dblack:{d_none:false,d_block:true},\r\n            subscr:{d_none:false,},\r\n            pass:\"password\",\r\n        }\r\n    },\r\n    methods:{\r\n        substr(){\r\n              if(this.uname&&this.password!=\"\"){\r\n            if(/^\\d{6,8}$/.test(this.uname)&/^\\d{6,8}$/.test(this.password)==true||/^1([38]\\d|5[0-35-9]|7[3678])\\d{8}$/.test(this.uname)&(this.reg==this.password)==true){\r\n            this.$messagebox({\r\n                title:\"登录\",\r\n                message:\"登录成功\",\r\n            }).then()\r\n            this.dnone.d_none=false;\r\n            this.dblack.d_block=false;\r\n            this.dblack.d_none=true;\r\n            this.dnone.d_block=true;\r\n            }else{\r\n                this.$messagebox({\r\n                    title:\"登录\",\r\n                    message:\"账号或密码错误\",\r\n                })\r\n            }\r\n            // this.dnone.d_none=true;\r\n            }else{\r\n                this.$messagebox({\r\n                    title:\"账号或密码不能为空\",\r\n                message:\"账号或密码不能为空\",\r\n                showConfirmButton:true,\r\n                showCancelButton:false,\r\n                })\r\n            }\r\n        },\r\n        //1添加methods\r\n        //2changeLoginMethod\r\n        changeLoginMethod(){\r\n            if(this.isUserAccount==true){\r\n                this.accountPlaceholder=\"手机账号\";\r\n                this.passwordPlaceholder=\"请输入验证码\";\r\n                this.label=\"验证码\";\r\n                this.qiehuan=\"手机登录\";\r\n                this.pass=\"text\";\r\n                this.isUserAccount=false;\r\n                this.buttonss.d_none=false;\r\n                this.buttonss.d_block=true;\r\n                this.uname=\"\";\r\n                this.password=\"\";\r\n            }else{\r\n                this.accountPlaceholder=\"手机/邮箱/会员名\";\r\n                this.passwordPlaceholder=\"请输入密码\";\r\n                this.label=\"密码\";\r\n                this.qiehuan=\"账号密码登录\";\r\n                this.pass=\"password\";\r\n                this.isUserAccount=true;\r\n                this.buttonss.d_none=true;\r\n                this.buttonss.d_block=false;\r\n                this.uname=\"\";\r\n                this.password=\"\";\r\n            }\r\n        },\r\n        get(){\r\n            this.reg=\"\";\r\n        for(var i=0;this.reg.length<4;i++){\r\n           var random=Math.floor(Math.random()*10);\r\n            this.reg+=random;\r\n            }\r\n             \r\n        this.$toast({\r\n            title:\"验证码\",\r\n            message:this.reg,\r\n            position:\"hiddle\",\r\n            duration:3100,\r\n            className:\"mytoast\"\r\n        })\r\n        \r\n        },\r\n        subcr(){\r\n          \r\n            this.$messagebox({\r\n                title:\"注销\",\r\n                message:\"是否确定注销\",\r\n                showConfirmButton:true,\r\n                showCancelButton:true,\r\n            }).then(a=>{\r\n               if(a==\"confirm\"){\r\n            this.dnone.d_none=true;\r\n            this.dnone.d_block=false;\r\n            this.dblack.d_none=false;\r\n            this.dblack.d_block=true;\r\n            this.uname=\"\";\r\n            this.password=\"\";\r\n            }\r\n            })\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style>\r\n    .text-align-center{\r\n        text-align: center;\r\n    }\r\n    .float-right{\r\n        float:right;\r\n        margin-top:-52px;\r\n        margin-right:15px;\r\n    }\r\n    .d_none{\r\n        display:none;\r\n    }\r\n    .d_block{\r\n        display:block;\r\n    }\r\n    .mytoast{\r\n        width:100px;\r\n        height:100px;\r\n        margin:0 auto;\r\n        line-height:60px;\r\n    }\r\n    .mint-toast-text{\r\n    font-size:25px !important;\r\n    }\r\n    .my-button{\r\n        font-size:6px;\r\n        line-height:6px;\r\n    }\r\n</style>"]}]}