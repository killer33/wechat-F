n            this.CurrentIndex[2].isSelect=false;\r\n            this.CurrentIndex[3].isSelect=false;\r\n            //再把点击的isSelect改为true\r\n            this.CurrentIndex[n].isSelect=true;\r\n            */\r\n\r\n            //1.创建循环\r\n            for(var i=0;i<this.CurrentIndex.length;i++){\r\n                //如果传进来的参数同于i 那么本次循环isSelect=true;\r\n                if(n==i){\r\n                    this.CurrentIndex[n].isSelect=true;\r\n                }else{\r\n                    //其他isSelect为false\r\n                    this.CurrentIndex[i].isSelect=false;\r\n                }\r\n            }\r\n                \r\n                \r\n        },\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.page-tabbar{\r\n    /* 溢出隐藏 */\r\n    overflow:hidden;\r\n}\r\n.mint-tabbar > .mint-tab-item{\r\n    color:#999;\r\n}\r\n.mint-tabbar>.mint-tab-item.is-selected{\r\n    background-color:transparent;\r\n    color:#45c018;\r\n}\r\n.page-wrap{\r\n    /* 溢出显示 */\r\n    overflow: auto;\r\n\r\n    padding-bottom: 60px;\r\n}\r\n</style>\r\n"]}]}